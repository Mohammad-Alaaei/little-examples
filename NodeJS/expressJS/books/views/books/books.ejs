<h2>Books List</h2>
<br>

<% if(locals.books){ %>
    <table border="1px">
        <th>ID</th>
        <th>Name</th>
        <th>Author</th>
        <th>Age</th>
        <th width="100%"></th>
        <th>Commands</th>

        <% books.forEach(itm=> { %>
            <tr>
                <td><%= itm.id %></td>
                <td><%= itm.name %></td>
                <td><%= itm.author %></td>
                <td><%= itm.age %></td>
                <td></td>
                <td>
                    <a href="#">Delete</a>
                    <a href="#">Edit</a>
                </td>
            </tr>
        <% }); %>
    </table>
<% } %>